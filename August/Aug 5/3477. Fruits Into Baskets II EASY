class Solution {
    public int numOfUnplacedFruits(int[] fruits, int[] baskets) {
        int n = fruits.length;
        int cnt = 0;

        for (int i = 0; i < n; i++) {
            int fruit = fruits[i];
            boolean pos = true;
            for (int j = 0; j < n; j++) {
                if (fruit <= baskets[j]) {
                    pos = false;
                    baskets[j] = 0;
                    break;
                }
            }
            if (pos)
                cnt++;
        }
        return cnt;
    }
}
