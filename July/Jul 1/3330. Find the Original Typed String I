class Solution {
    public int possibleStringCount(String word) {
        int n = word.length();
        int i = 0;
        int res = 0;

        while(i < n) {
            char ch = word.charAt(i);
            int count = 0;
            while(i < n && ch == word.charAt(i)) {
                count++;
                i++;
            }
            res += count - 1;
        }

        return res + 1;
    }
}