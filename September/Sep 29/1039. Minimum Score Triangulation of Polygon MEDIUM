class Solution {
    public int minScoreTriangulation(int[] values) {
        int n = values.length;
        int[][] dp = new int[n + 1][n + 1];
        for (int[] t : dp)
            Arrays.fill(t, -1);

        return solve(values, 0, n - 1, dp);
    }

    int solve(int[] val, int i, int j, int[][] dp) {
        if (j - i + 1 < 3)
            return 0;
        if (dp[i][j] != -1)
            return dp[i][j];
        
        int res = Integer.MAX_VALUE;

        for (int k = i + 1; k < j; k++) {
            int wt = val[i] * val[j] * val[k] + solve(val, i, k, dp) + solve(val, k, j, dp);
            res = Math.min(res, wt);
        }

        return dp[i][j] = res;
    }
}
