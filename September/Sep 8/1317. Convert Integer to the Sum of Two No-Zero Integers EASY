class Solution {
    public int[] getNoZeroIntegers(int n) {
        int x = 1;
        int y = n - 1;
        boolean not = true;
        
        while (x <= y) {
            if(check(x) || check(y)) {
                x++;
                y--;
            } else {
                break;
            }
        }

        return new int[]{x, y};
    }

    boolean check(int num) {
        if (num == 0)
            return true;
        while(num > 0) {
            if (num % 10 == 0)
                return true;
            num /= 10;
        }
        return false;
    }
}
